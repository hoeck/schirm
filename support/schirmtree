"""

node_link_tree_js = r"""
function buildTree() {
  var r = 960 / 2;

  var tree = d3.layout.tree()
      .size([360, r - 120])
      .separation(function(a, b) { return (a.parent == b.parent ? 1 : 2) / a.depth; });

  var diagonal = d3.svg.diagonal.radial()
      .projection(function(d) { return [d.y, d.x / 180 * Math.PI]; });
  
  var vis = d3.select("#tree-container").append("svg:svg")
      .attr("width", r * 2)
      .attr("height", r * 2 - 150)
      .append("svg:g")
      .attr("transform", "translate(" + r + "," + r + ")");

  d3.json("tree.json", function(json) {
    var nodes = tree.nodes(json);

    var link = vis.selectAll("path.link")
        .data(tree.links(nodes))
        .enter().append("svg:path")
        .attr("class", "link")
        .attr("d", diagonal);

    var node = vis.selectAll("g.node")
        .data(nodes)
        .enter().append("svg:g")
        .attr("class", "node")
        .attr("transform", function(d) { return "rotate(" + (d.x - 90) + ")translate(" + d.y + ")"; })

    node.append("svg:circle")
        .attr("class", 'node-circle')
        .attr("r", 4.5);

    node.append("svg:text")
        .attr("class", "node-text")
        .attr("dx", function(d) { return d.x < 180 ? 8 : -8; })
        .attr("dy", ".31em")
        .attr("text-anchor", function(d) { return d.x < 180 ? "start" : "end"; })
        .attr("transform", function(d) { return d.x < 180 ? null : "rotate(180)"; })
        .text(function(d) { return d.name; });
  });
}
"""

html = """
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <style>
        #tree-container {
            width: 100%;
        }

        .link {
            fill: none;
            stroke: #ccc;
            stroke-width: 1.5px;
        }

        .node-text {
          font-size: 10px;
          font-family: sans-serif;
        }
    </style>
    <style>
        .node-circle {
          fill: #fff;
          stroke: steelblue;
          stroke-width: 1.5px;
        }
    </style>
</head>
<body>

<div id="tree-container"></div>

<script type="text/javascript" src="jquery.js"></script>

<script type="text/javascript" src="d3.min.js"></script>
<script type="text/javascript" src="d3.layout.min.js"></script>

<script type="text/javascript" src="nodeLinkTree.js"></script>

<script>
    $(function() {
      buildTree();
      schirmlog("done");
    });
</script>

</body>
</html>
"""

def create_tree(root=".", filter_f=lambda x: True):
    if os.path.isdir(root):
        _, name = os.path.split(root)
        return {'name': name,
                'children': [create_tree(os.path.join(root, x), filter_f)
                             for x in sorted(os.listdir(root))
                             if filter_f(x)]}
    else:
        _, name = os.path.split(root)
        return {'name': name}

def show_tree(tree):
    with schirm.frame():
        # schirm.register_resource('../misc/jquery-1.6.2.js')
        # schirm.register_resource('../misc/d3.min.js')
        # schirm.register_resource('../misc/d3.layout.min.js')
        # schirm.register_resource('../misc/nodeLinkTree.js')
        schirm.register_resource_data(jquery_1_7_min_js, 'jquery.js')
        schirm.register_resource_data(d3_min_js, 'd3.min.js')
        schirm.register_resource_data(d3_layout_min_js, 'd3.layout.min.js')
        schirm.register_resource_data(node_link_tree_js, 'nodeLinkTree.js')
        schirm.register_resource_data(json.dumps(tree), 'tree.json', 'application/json')
        print html
        schirm.close()
        # wait for the 'done' message
        reqtype, msg = schirm.read_next()

def main():
    parser = argparse.ArgumentParser(description="Display the current dir or PATH as a node link tree")
    parser.add_argument("path", metavar="PATH", default=".", nargs="?")
    args = parser.parse_args()
    
    show_tree(create_tree(os.path.abspath(args.path), lambda x: not os.path.split(x)[1].startswith(".")))


if __name__ == '__main__':
    main()
